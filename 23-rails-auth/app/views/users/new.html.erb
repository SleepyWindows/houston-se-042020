<h1>Create User</h1>

<%= form_with model: @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Create User" %>
<% end %>


<% flash.each do |msgs| %>
  <% msgs.each do |msg| %>
    <p><%= msg %></p>
  <% end %>
<% end %>